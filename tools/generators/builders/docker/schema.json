{
  "$schema": "http://json-schema.org/schema",
  "type": "object",
  "properties": {
    "registry": {
      "type": "string",
      "description": "Gitlab registry to push the docker image",
      "default": "ghcr.io/sonnymiel/sonnymiel.com"
    },
    "version": {
      "type": "string",
      "description": "Version to push to github registry"
    }
  },
  "additionalProperties": false,
  "required": ["registry"]
}